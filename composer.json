{
    "name": "drobee/nova-sluggable",
    "description": "Slug field for Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*|7.*|8.*",
        "illuminate/http": "5.6.*|5.7.*|5.8.*|6.*|7.*|8.*"
    },
    "autoload": {
        "psr-4": {
            "Drobee\\NovaSluggable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Drobee\\NovaSluggable\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
